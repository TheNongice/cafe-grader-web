= render partial: 'toggle_button',
                  locals: {button_id: "#problem-avail-#{@problem.id}",button_on: @problem.available }
:plain
  r = $("#prob-#{@problem.id}");
  r.removeClass('bg-success');
  r.removeClass('bg-danger');
  r.addClass("#{@problem.available? ? 'bg-success' : 'bg-danger-'}");

